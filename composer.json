{
    "name": "eloise/laravel-data-audit",
    "description": "This laravel package help us to register every model activities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "eloise",
            "email": "eloise@example.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "symfony/finder": "^6|^7",
        "phpunit/phpunit": "^11.3",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.3",
        "laravel/prompts": "^0.1.24"
    },
    "autoload": {
        "psr-4": {
            "Eloise\\DataAudit\\": "src/",
            "Eloise\\DataAudit\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Eloise\\DataAudit\\DataAuditServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Eloise\\DataAudit\\Facade"
            }
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpro/grumphp": "^2.6",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
